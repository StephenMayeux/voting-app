extends layout

block content
  h3= title
  form(method="POST", action="/signin")
    div
      p Email: 
        input(type="email", name="email")
      p Password: 
        input(type="password", name="password")
      p
        input(type="submit")
      br
  .center
    p Don't have an account?
    a(href='/signup') Sign Up
