extends layout

block content
  h1 Voting App
  h3= title
  form(method="POST", action="/signin")
    p Email: 
      input(type="email", name="email")
    p Password: 
      input(type="password", name="password")
    p
      input(type="submit")
    br
    p Don't have an account?
    a(href='/signup') Sign Up
    br
    br
    a(href='/') Return to home
