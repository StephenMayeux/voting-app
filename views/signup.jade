extends layout

block content
  h1 Voting App
  h3= title
  form(method="POST", action="/signup")
    p Email: 
      input(type="email", name="email")
    p Name you wish to use:
      input(type="text", name="username")
    p Password:
      input(type="password", name="password")
    p
      input(type="submit")
    br 
    a(href='/') Return to home

